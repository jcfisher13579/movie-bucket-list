<section class="card">
  <a href="/movie/{{rank}}">
    <header>
      <h2>{{title}}</h2>
    </header>
    <img src="{{movie.[0].image}}" alt="{{movie.[0].title}}">
  </a>
  <div>
    <input type="checkbox" id="watched" name="watched" value="movie.[0]">
    <label for="vehicle1">Watched?</label><br>
    <input type="checkbox" id="willwatch" name="reviewed" value="movie.[0]">
    <label for="vehicle2">Reviewed?</label><br>
  </div>
</section>
<script src="/public/js/checkbox.js"></script>
